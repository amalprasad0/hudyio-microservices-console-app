{
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:7074/"
  },
  "Routes": [
    {
      "UpstreamPathTemplate": "/gateway/health",
      "UpstreamHttpMethod": [ "Get" ],
      "DownstreamPathTemplate": "/api/User/health",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "host.docker.internal",
          "Port": 4202
        }
      ]
    },
    {
      "UpstreamPathTemplate": "/gateway/storeuserandsendotp",
      "UpstreamHttpMethod": [ "Post"],
      "DownstreamPathTemplate": "/api/user/createuser",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "host.docker.internal",
          "Port": 4202
        }
      ]
    },
    {
      "UpstreamPathTemplate": "/gateway/chat-hub",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE", "OPTIONS" ],
      "DownstreamPathTemplate": "/chat-hub",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "host.docker.internal",
          "Port": 4203
        }
      ],
      

    }
  ]
}
